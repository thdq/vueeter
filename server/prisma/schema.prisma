generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
  
}

model User {
  id         String   @id @default(uuid())
  username   String   @unique
  birth_date DateTime
  email      String   @unique
  name       String
  password   String
   @@map(name: "user")
}
